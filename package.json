{
  "name": "objective-moxygen",
  "version": "0.8.0",
  "description": "Doxygen XML to Markdown documentation converter for ObjC",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "release": "release-it",
    "test": "npm run clean && cd example/ && doxygen && cd .. && node bin/objective-moxygen.js --output=example/doc/test.md example/xml",
    "clean": "rm -rf example/doc/*.md example/xml/"
  },
  "bin": {
    "objective-moxygen": "bin/objective-moxygen.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arantes555/objective-moxygen.git"
  },
  "keywords": [
    "doxygen",
    "markdown",
    "documentation",
    "generator"
  ],
  "author": "arantes555",
  "license": "MIT",
  "dependencies": {
    "commander": "^10.0.0",
    "handlebars": "^4.7.7",
    "object-assign": "^4.1.1",
    "winston": "^3.8.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "standard": "^17.0.0"
  }
}
